var map = fun(func, coll) {
    var iterable = fun(collec, procd) {
        if (len(collec) == 0) {
            ret procd;
        } else {
            ret iterable(rest(collec), push(procd, func(first(collec))));
        }
    };
    ret iterable(coll, []);
};

var str_concat = fun(lhs, rhs) {
    ret string(lhs) + string(rhs);
};

var file_read = fun(path) {
    var lines = file_read_lines(path);
    ret str_join(lines, " ");
};